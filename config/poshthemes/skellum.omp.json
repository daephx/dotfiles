{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Segments.Git.RepoName}} {{.Segments.Git.RepoName}} {{else}} {{.Folder}} {{end}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "foreground": "#757575",
          "style": "plain",
          "template": "\u250c"
        },
        {
          "foreground": "#fafafa",
          "style": "plain",
          "template": " {{.Icon}}<#7A7A7A> \ue0b1</>",
          "type": "os",
          "properties": {
            "alpine": "<#0f5779>\uF300</>",
            "aosc": "<#fafafa>\uF301</>",
            "arch": "<#1b90c8>\uF303</>",
            "centos": "<#fafafa>\uF304</>",
            "coreos": "<#fafafa>\uF305</>",
            "debian": "<#d70a53>\uF306</>",
            "devuan": "<#fafafa>\uF307</>",
            "elementary": "<#fafafa>\uF309</>",
            "fedora": "<#fafafa>\uF30a</>",
            "gentoo": "<#fafafa>\uF30d</>",
            "linux": "<#fafafa>\uF17C</>",
            "macos": "<#fafafa>\uF179</>",
            "mageia": "<#fafafa>\uF310</>",
            "manjaro": "<#fafafa>\uF312</>",
            "mint": "<#fafafa>\uF30e</>",
            "nixos": "<#fafafa>\uF313</>",
            "opensuse": "<#fafafa>\uF314</>",
            "raspbian": "<#bc1a48>\uF315</>",
            "sabayon": "<#fafafa>\uF317</>",
            "slackware": "<#556fb3>\uF319</>",
            "ubuntu": "<#E95420>\uF31b</>",
            "windows": "<#26C6DA>\uE62A</>"
          }
        },
        {
          "foreground": "#ffff66",
          "properties": {
            "root_icon": "\uf0e7"
          },
          "style": "powerline",
          "template": " \uf0e7<#7A7A7A> \ue0b1</>",
          "type": "root"
        },
        {
          "foreground": "#fafafa",
          "properties": {
            "folder_icon": "\u2026",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": " {{.Path}}",
          "type": "path"
        },
        {
          "foreground": "red",
          "style": "powerline",
          "template": "{{if not .Writable}} \uf023{{end}}",
          "type": "path"
        },
        {
          "foreground": "yellow",
          "foreground_templates": [
            "{{if or (.Working.Changed) (.Staging.Changed)}}yellow{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}red{{ end }}",
            "{{if gt .Ahead 0}}magenta{{end}}",
            "{{if gt .Behind 0}}cyan{{end}}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " <#7A7A7A>\ue0b1 </>{{.UpstreamIcon}}{{.HEAD}}{{.BranchStatus}}{{if .Working.Changed}} \uf044 <#E84855>{{.Working.String}}</>{{end}}{{if and (.Staging.Changed) (.Working.Changed)}} |{{end}}{{if .Staging.Changed}} \uf046 <#2FDA4E>{{.Staging.String}}</>{{end}}{{if gt .StashCount 0}} \uf692 {{.StashCount}}{{end}}{{if gt .WorktreeCount 0}} \uf1bb {{.WorktreeCount}}{{end}} ",
          "type": "git"
        }
      ]
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "terraform",
          "background": "#ffee58",
          "foreground": "#100e23",
          "style": "powerline",
          "template": " {{ .WorkspaceName }}{{ if .Version }} {{ .Version }}{{ end }} "
        },
        {
          "foreground": "#576eff",
          "style": "powerline",
          "template": " \ue620 {{.Full}}<#7A7A7A> \ue0b3</>",
          "type": "lua"
        },
        {
          "foreground": "#3069DE",
          "style": "powerline",
          "template": " \ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "kubectl"
        },
        {
          "foreground": "#FFA400",
          "style": "powerline",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "aws"
        },
        {
          "foreground": "#9ec3f0",
          "properties": {
            "display_id": true,
            "display_name": true,
            "info_separator": " @ "
          },
          "style": "powerline",
          "template": " \ufd03 {{.Name}}<#7A7A7A> \ue0b3</>",
          "type": "az"
        },
        {
          "foreground": "#FEAC19",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \uf0e7 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "azfunc"
        },
        {
          "foreground": "#06A4CE",
          "style": "powerline",
          "template": " \ue798 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "dart"
        },
        {
          "foreground": "#4063D8",
          "style": "powerline",
          "template": " \ue624 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "julia"
        },
        {
          "foreground": "#B7410E",
          "style": "powerline",
          "template": " \ue7a8 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "rust"
        },
        {
          "foreground": "#f89820",
          "style": "powerline",
          "template": " \ue738 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "java"
        },
        {
          "foreground": "#7FD5EA",
          "style": "powerline",
          "template": " \ufcd1 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "go"
        },
        {
          "foreground": "#EC0000",
          "style": "powerline",
          "template": " \ue791 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "ruby"
        },
        {
          "foreground": "#6CA35E",
          "style": "powerline",
          "template": " \ue718 {{if .PackageManagerIcon}}{{.PackageManagerIcon}} {{end}}{{.Full}}<#7A7A7A> \ue0b3</>",
          "type": "node"
        },
        {
          "foreground": "#96E072",
          "style": "powerline",
          "template": " \ue235 {{if .Error}}{{.Error}}{{else}}{{if .Venv}}{{.Venv}} {{end}}{{.Full}}{{end}}<#7a7a7a> \ue0b3</>",
          "properties": {
            "display_mode": "files"
          },
          "type": "python"
        },
        {
          "foreground": "#3891A6",
          "style": "powerline",
          "template": " \ue77f {{if .Unsupported}}\uf071{{else}}{{.Full}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "dotnet"
        },
        {
          "foreground": "#ffffff",
          "style": "powerline",
          "template": " \ufcb5 {{.Name}}",
          "type": "shell"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "foreground": "#757575",
          "style": "plain",
          "template": "\u2514"
        },
        {
          "foreground": "lightGreen",
          "foreground_templates": ["{{if gt .Code 0}}red{{end}}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f",
          "type": "exit"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 2
}
