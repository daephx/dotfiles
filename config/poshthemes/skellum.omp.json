{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{if .Segments.Git.RepoName}} {{.Segments.Git.RepoName}} {{else}} {{.Folder}} {{end}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "8",
          "template": "\u250c"
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "#fafafa",
          "template": " {{.Icon}}<8> \ue0b1</>",
          "properties": {
            "alpine": "<#0f5779>\uf300</>",
            "aosc": "<#fafafa>\uf301</>",
            "arch": "<#1b90c8>\uf303</>",
            "centos": "<#fafafa>\uf304</>",
            "coreos": "<#fafafa>\uf305</>",
            "debian": "<#d70a53>\uf306</>",
            "devuan": "<#fafafa>\uf307</>",
            "elementary": "<#526ec8>\uf309</>",
            "fedora": "<#52a5d8>\uf30a</>",
            "gentoo": "<#918ccb>\uf30d</>",
            "linux": "<#fafafa>\uf17c</>",
            "macos": "<#fafafa>\uf179</>",
            "mageia": "<#425979>\uf310</>",
            "manjaro": "<#39b860>\uf312</>",
            "mint": "<#83b74d>\uf30e</>",
            "nixos": "<#5075ba>\uf313</>",
            "opensuse": "<#71b337>\uf314</>",
            "raspbian": "<#bc1a48>\uf315</>",
            "sabayon": "<#fafafa>\uf317</>",
            "slackware": "<#556fb3>\uf319</>",
            "ubuntu": "<#E95420>\uf31b</>",
            "windows": "<#26C6DA>\ue62a</>"
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "foreground": "lightYellow",
          "template": " \uf0e7<8> \ue0b1</>",
          "properties": {
            "root_icon": "\uf0e7"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "lightBlue",
          "template": " {{.Path}}",
          "properties": {
            "folder_icon": "\u2026",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "style": "agnoster_short"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "red",
          "template": "{{if not .Writable}} \uf023{{end}}"
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "yellow",
          "foreground_templates": [
            "{{if or (.Working.Changed) (.Staging.Changed)}}yellow{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}red{{ end }}",
            "{{if gt .Ahead 0}}magenta{{end}}",
            "{{if gt .Behind 0}}cyan{{end}}"
          ],
          "template": " <8>\ue0b1 </>{{.UpstreamIcon}}{{.HEAD}}{{.BranchStatus}}{{if .Working.Changed}} \uf044 <red>{{.Working.String}}</>{{end}}{{if and (.Staging.Changed) (.Working.Changed)}} |{{end}}{{if .Staging.Changed}} \uf046 <#2FDA4E>{{.Staging.String}}</>{{end}}{{if gt .StashCount 0}} \udb80\udd93 {{.StashCount}}{{end}}{{if gt .WorktreeCount 0}} \uf1bb {{.WorktreeCount}}{{end}} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "terraform",
          "style": "powerline",
          "foreground": "93",
          "template": " {{ .WorkspaceName }}{{ if .Version }} {{ .Version }}{{ end }} "
        },
        {
          "type": "lua",
          "style": "powerline",
          "foreground": "74",
          "template": " \ue620 {{.Full}}<8> \ue0b3</>"
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "foreground": "62",
          "template": " \udb82\udc33 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "aws",
          "style": "powerline",
          "foreground": "214",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "az",
          "style": "powerline",
          "foreground": "153",
          "template": " \ufd03 {{.Name}}<8> \ue0b3</>",
          "properties": {
            "display_id": true,
            "display_name": true,
            "info_separator": "  "
          }
        },
        {
          "type": "azfunc",
          "style": "powerline",
          "foreground": "214",
          "template": " \uf0e7 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<8> \ue0b3</>",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          }
        },
        {
          "type": "dart",
          "style": "powerline",
          "foreground": "25",
          "template": " \ue798 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "julia",
          "style": "powerline",
          "foreground": "133",
          "template": " \ue624 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "rust",
          "style": "powerline",
          "foreground": "166",
          "template": " \ue7a8 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "java",
          "style": "powerline",
          "foreground": "208",
          "template": " \ue738 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "go",
          "style": "powerline",
          "foreground": "74",
          "template": " \udb81\udfd3 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "ruby",
          "style": "powerline",
          "foreground": "160",
          "template": " \ue791 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "node",
          "style": "powerline",
          "foreground": "71",
          "template": " \ue718 {{if .PackageManagerIcon}}{{.PackageManagerIcon}} {{end}}{{.Full}}<8> \ue0b3</>"
        },
        {
          "type": "python",
          "style": "powerline",
          "foreground": "214",
          "template": " \ue235 {{if .Error}}{{.Error}}{{else}}{{if .Venv}}{{.Venv}} {{end}}{{.Full}}{{end}}<8> \ue0b3</>",
          "properties": {
            "display_mode": "context"
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "foreground": "69",
          "template": " \ue77f {{if .Unsupported}}\uf071{{else}}{{.Full}}{{end}}<8> \ue0b3</>"
        },
        {
          "type": "shell",
          "style": "powerline",
          "foreground": "15",
          "template": " \udb81\udfb7 {{.Name}}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "8",
          "template": "\u2514"
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "2",
          "foreground_templates": ["{{if gt .Code 0}}1{{end}}"],
          "template": "❯",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "newline": true
    }
  ]
}
