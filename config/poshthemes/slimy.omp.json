{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#2f2f2f",
          "foreground": "#26C6DA",
          "leading_diamond": "\ue0c7\u2593",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f",
            "wsl": "\ue712",
            "wsl_separator": " - "
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}<#7A7A7A> \ue0b1</>",
          "type": "os"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#fafafa",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": "{{ if .SSHSession }}\uf817 {{ end }}<#77f5d6>{{ .UserName }}</><#7A7A7A>\uf1fa</><#2EEFBF>{{ .HostName }}</><#7A7A7A> \ue0b1</>",
          "type": "session"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#ffff66",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "root_icon": "\uf0e7"
          },
          "style": "powerline",
          "template": " \uf0e7<#7A7A7A> \ue0b1</>",
          "type": "root"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#fafafa",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\ue5fe ",
            "folder_separator_icon": "\ue0bf ",
            "home_icon": "\uf015 ",
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#ffeb3b",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffea00{{ end }}",
            "{{ if gt .Ahead 0 }}#2EC4B6{{ end }}",
            "{{ if gt .Behind 0 }}#8A4FFF{{ end }}"
          ],
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": "<#7a7a7a>\ue0b1 </>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<#E84855> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#2FDA4E> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#2f2f2f",
          "foreground": "#3069DE",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "kubectl"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#FFA400",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#7A7A7A> \ue0b3</>",
          "type": "aws"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#9ec3f0",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_id": true,
            "display_name": true,
            "info_separator": " @ "
          },
          "style": "powerline",
          "template": " \ufd03 {{ .Name }}<#7A7A7A> \ue0b3</>",
          "type": "az"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#FEAC19",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \uf0e7 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "azfunc"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#06A4CE",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "dart"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#4063D8",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "julia"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#B7410E",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "rust"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#f89820",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "java"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#7FD5EA",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "go"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#EC0000",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "ruby"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#6CA35E",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#7A7A7A> \ue0b3</>",
          "type": "node"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#96E072",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "python"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#3891A6",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#7A7A7A> \ue0b3</>",
          "type": "dotnet"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \ufcb5 {{ .Name }}<#7A7A7A> \ue0b3</>",
          "type": "shell"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#fafafa",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": "{{ .CurrentDate | date .Format }}<#007ACC> \uf64f </>",
          "trailing_diamond": "\u2593\ue0c6",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#9FD356",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E84855{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "final_space": true,
  "version": 2
}
